---
interface Props { name: string; description: string; thumbnailUrl: string; uploadDate: string; contentUrl?: string; embedUrl?: string; duration?: string }
const p = Astro.props as Props;
const data = {"@context":"https://schema.org","@type":"VideoObject",name:p.name,description:p.description,thumbnailUrl:p.thumbnailUrl,uploadDate:p.uploadDate,contentUrl:p.contentUrl,embedUrl:p.embedUrl,duration:p.duration};
---
<script type="application/ld+json">{JSON.stringify(data)}</script>
