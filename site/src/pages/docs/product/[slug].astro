---
export function getStaticPaths() {
	const map: Record<string,string> = {
		'aria': '/products/aria-shield/',
		'gateway': '/products/gateway/',
		'pdp': '/products/pdp/',
		'idp': '/products/idp/',
		'registry': '/products/registry/',
		'receipts': '/products/receipts/',
		'membership': '/products/membership/',
		'orchestration': '/products/orchestration/',
	};
	return Object.keys(map).map((slug) => ({ params: { slug } }));
}
const mapping: Record<string,string> = {
	'aria': '/products/aria-shield/',
	'gateway': '/products/gateway/',
	'pdp': '/products/pdp/',
	'idp': '/products/idp/',
	'registry': '/products/registry/',
	'receipts': '/products/receipts/',
	'membership': '/products/membership/',
	'orchestration': '/products/orchestration/',
};
const slug = Astro.params.slug as string;
const to = mapping[slug] || '/products/';
---
<html>
	<head>
		<meta http-equiv="refresh" content={`0; url=${to}`} />
		<link rel="canonical" href={to} />
	</head>
	<body>
		<a href={to}>Redirectingâ€¦</a>
	</body>
</html>
